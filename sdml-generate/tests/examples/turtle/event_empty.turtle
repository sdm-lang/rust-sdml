        sdml:startLine 5 ;
        sdml:endLine 5 ;
        sdml:startColumn 3 ;
        sdml:endColumn 36 ;
        sdml:startByte 40 ;
        sdml:endByte 73 ;
    ] ;
    rdf:type
        owl:Class ,
        sdml:Event ;
    sdml:hasSourceEntity campaign:Campaign ;
    sdml:srcLabel "NameChanged" ;
    rdfs:isDefinedBy campaign: .

