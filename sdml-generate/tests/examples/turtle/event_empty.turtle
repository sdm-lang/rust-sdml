@prefix sdml: <http://sdml.io/sdml-owl.ttl#> .

campaign:
    sdml:hasSourceLocation [
        sdml:startLine 1 ;
        sdml:endLine 8 ;
        sdml:startColumn 1 ;
        sdml:endColumn 1 ;
        sdml:startByte 0 ;
        sdml:endByte 79 ;
    ] ;
