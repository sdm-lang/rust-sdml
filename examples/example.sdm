module example <https://example.com/api> is

  import [ dc xsd ]

  datatype Uuid <- string is
    @xsd:pattern = "[0-9a-f]{8}-([0-9a-f]{4}-){3}[0-9a-f]{12}"
  end

  entity Example is
    identity example_id -> Uuid

    version -> Uuid
    name -> string is
      @dc:description = "the name of this thing"@en
    end
  end

end
